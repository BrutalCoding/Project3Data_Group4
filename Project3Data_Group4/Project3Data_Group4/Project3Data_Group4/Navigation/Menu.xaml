<?xml version="1.0" encoding="UTF-8" ?> 
<MasterDetailPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NSContacten;assembly=NSContacten"
    x:Class="Project3Data_Group4.Menu"
    Title="Master Detail Menu">

   <MasterDetailPage.Master>
        <ContentPage Title="Master" BackgroundColor="Transparent" >
            <!-- Master Page -->
            <Label HorizontalTextAlignment="Center" VerticalTextAlignment="Center">Master</Label>
        </ContentPage>
    </MasterDetailPage.Master>
 
    <MasterDetailPage.Detail>
        <ContentPage BackgroundColor="Silver">
            <!-- Detail Page -->
            <Label HorizontalTextAlignment="Center" VerticalTextAlignment="Center">Detail</Label>
            
        </ContentPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>